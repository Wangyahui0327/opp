<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/toolbar6"
        android:layout_width="409dp"
        android:layout_height="wrap_content"
        android:background="?attr/colorPrimary"
        android:minHeight="?attr/actionBarSize"
        android:theme="?attr/actionBarTheme"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/imageViewPopulation6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="population"
        android:textStyle="bold"
        android:gravity="center"
        android:drawableStart="@android:drawable/star_on"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/textViewPopulation6"
        app:layout_constraintStart_toStartOf="@+id/textViewPopulation6"
        app:layout_constraintTop_toBottomOf="@+id/toolbar6"
        app:layout_constraintVertical_bias="0.18" />

    <TextView
        android:id="@+id/city1Tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Population"
        android:textStyle="bold"
        android:textColor="@color/black"
        android:layout_marginBottom="20dp"
        app:layout_constraintBottom_toTopOf="@+id/imageViewPopulation6"
        app:layout_constraintStart_toStartOf="@+id/textViewPopulation6"
        app:layout_constraintEnd_toEndOf="@+id/textViewPopulation6"  />

    <TextView
        android:id="@+id/textViewPopulation6"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:text="Population"
        android:gravity="center"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/textViewPopulationComparison6"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageViewPopulation6"
        app:layout_constraintVertical_bias="0" />

    <TextView
        android:id="@+id/imageViewSurface6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="WorkPlaceSelfSufufficiency"
        android:textStyle="bold"
        android:gravity="center"
        android:drawableStart="@android:drawable/star_on"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/imageViewPopulation6"
        app:layout_constraintStart_toStartOf="@+id/imageViewPopulation6"
        app:layout_constraintTop_toBottomOf="@+id/textViewPopulation6"
        app:layout_constraintVertical_bias="0.18" />

    <TextView
        android:id="@+id/textViewSurface6"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:text="Surface"
        android:gravity="center"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/textViewSurfaceComparison6"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageViewSurface6"
        app:layout_constraintVertical_bias="0.0" />



























    <TextView
        android:id="@+id/imageRates7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Employment Rates"
        android:drawableStart="@android:drawable/star_on"
        android:gravity="center"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/imageViewPopulation6"
        app:layout_constraintStart_toStartOf="@+id/imageViewPopulation6"
        app:layout_constraintTop_toBottomOf="@+id/textViewSurface6"
        app:layout_constraintVertical_bias="0.18" />

    <TextView
        android:id="@+id/textEmploymentRates7"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:text="Employment Rates"
        android:gravity="center"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/textViewEmploymentComparison6"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageRates7"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/imageViewWeather6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Weather"
        android:textStyle="bold"
        android:gravity="center"
        android:drawableStart="@android:drawable/star_on"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/imageViewPopulation6"
        app:layout_constraintStart_toStartOf="@+id/imageViewPopulation6"
        app:layout_constraintTop_toBottomOf="@+id/textEmploymentRates7"
        app:layout_constraintVertical_bias="0.18"
        app:srcCompat="@android:drawable/star_off" />

    <TextView
        android:id="@+id/textViewWeather6"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:text="Weather"
        android:gravity="center"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/textViewWeatherComparison6"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageViewWeather6"
        app:layout_constraintVertical_bias="0.0" />


    <TextView
        android:id="@+id/city2Tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Population"
        android:textStyle="bold"
        android:textColor="@color/black"
        android:layout_marginBottom="20dp"
        app:layout_constraintTop_toTopOf="@+id/city1Tv"
        app:layout_constraintStart_toStartOf="@+id/textViewPopulationComparison6"
        app:layout_constraintEnd_toEndOf="@+id/textViewPopulationComparison6"  />

    <TextView
        android:id="@+id/textViewPopulationComparison6"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="30dp"
        android:text="the population comparison"
        android:gravity="center"
        app:layout_constraintBottom_toBottomOf="@+id/textViewPopulation6"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/textViewPopulation6"
        app:layout_constraintTop_toTopOf="@+id/textViewPopulation6"
        app:layout_constraintVertical_bias="0.5" />

    <TextView
        android:id="@+id/textViewSurfaceComparison6"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="30dp"
        android:text="the surface comparison"
        android:gravity="center"
        app:layout_constraintBottom_toBottomOf="@+id/textViewSurface6"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textViewSurface6"
        app:layout_constraintTop_toTopOf="@+id/textViewSurface6" />

    <TextView
        android:id="@+id/textViewEmploymentComparison6"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="30dp"
        android:text="employment comparison"
        android:gravity="center"
        app:layout_constraintBottom_toBottomOf="@+id/textEmploymentRates7"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textEmploymentRates7"
        app:layout_constraintTop_toTopOf="@+id/textEmploymentRates7" />




    <TextView
        android:id="@+id/textViewWeatherComparison6"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="30dp"
        android:text="the weather comparison"
        android:gravity="center"
        app:layout_constraintBottom_toBottomOf="@+id/textViewWeather6"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textViewWeather6"
        app:layout_constraintTop_toTopOf="@+id/textViewWeather6" />

    <TextView
        android:id="@+id/textViewScreen6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Comparison"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        app:layout_constraintBottom_toBottomOf="@+id/toolbar6"
        app:layout_constraintEnd_toEndOf="@+id/toolbar6"
        app:layout_constraintStart_toStartOf="@+id/toolbar6"
        app:layout_constraintTop_toTopOf="@+id/toolbar6" />

    <TextView
        android:id="@+id/loadDataTv"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@color/white"
        android:gravity="center"
        android:text="loading..."
        android:visibility="visible"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/toolbar6"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintBottom_toBottomOf="parent" />

    <ImageButton
        android:id="@+id/imageButtonBack6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/toolbar6"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="?attr/actionModeCloseDrawable" />


</androidx.constraintlayout.widget.ConstraintLayout>